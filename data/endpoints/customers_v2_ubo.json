      "CreateUboNaturalDetailsRequest": {
        "required": [
          "countryOfResidence",
          "dateOfBirth",
          "firstName",
          "surname"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "minLength": 1,
            "type": "string",
            "description": "The UBO's first name.",
            "example": "Linn"
          },
          "middleName": {
            "minLength": 1,
            "type": "string",
            "description": "The UBO's middle name.",
            "nullable": true,
            "example": "Hernan"
          },
          "surname": {
            "minLength": 1,
            "type": "string",
            "description": "The UBO's surname.",
            "example": "Sanches"
          },
          "countryOfResidence": {
            "$ref": "#/components/schemas/CountryCode"
          },
          "dateOfBirth": {
            "type": "string",
            "description": "The UBO's date of birth, formatted yyyy-MM-dd.",
            "format": "date",
            "example": "1978-05-22"
          }
        },
        "additionalProperties": false,
        "description": "Personal details for a natural person UBO. Must only be provided if the uboType is 'NaturalPerson'."
      },
      "CreateUboNonNaturalDetailsRequest": {
        "required": [
          "countryOfRegistration",
          "registeredName"
        ],
        "type": "object",
        "properties": {
          "registeredName": {
            "minLength": 1,
            "type": "string",
            "description": "The registered name of the company.",
            "example": "Smitty and Co"
          },
          "countryOfRegistration": {
            "$ref": "#/components/schemas/CountryCode"
          }
        },
        "additionalProperties": false,
        "description": "Business details for a company UBO. Must only be provided if the uboType is 'NonNaturalPerson'."
      },
      "CreateUboRequest": {
        "required": [
          "customerId",
          "uboType",
          "ownershipPercentage"
        ],
        "type": "object",
        "properties": {
          "customerId": {
            "type": "string",
            "description": "The ID of the legal entity to link this new UBO to.",
            "format": "uuid",
            "example": "a52e831a-5203-579f-bd54-fc77842a4d51"
          },
          "ownershipPercentage": {
            "type": "integer",
            "description": "The ownership percentage the UBO has of the associated legal entity.",
            "format": "int32",
            "nullable": true,
            "example": 100
          },
          "uboType": {
            "$ref": "#/components/schemas/CreateUboTypeRequest"
          },
          "personalDetails": {
            "$ref": "#/components/schemas/CreateUboNaturalDetailsRequest"
          },
          "businessDetails": {
            "$ref": "#/components/schemas/CreateUboNonNaturalDetailsRequest"
          }
        },
        "additionalProperties": false,
        "description": "Request body for creating an Ultimate Beneficial Owner (UBO)."
      },
      "CreateUboTypeRequest": {
        "enum": [
          "NaturalPerson",
          "NonNaturalPerson"
        ],
        "type": "string",
        "description": "Used to specify whether you are creating a natural or non-natural UBO."
      },
      "CreatedResourceResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique ID for the created UBO. You should retain this ID for future reference.",
            "format": "uuid"
          }
        },
        "additionalProperties": false,
        "description": "Response containing information about the created resource"
      },
      "CustomerStatus": {
        "enum": [
          "Active"
        ],
        "type": "string",
        "example": "Active",
        "description": "Represents the status of a customer."
      },
      "CustomerType": {
        "enum": [
          "UboNatural",
          "UboNonNatural"
        ],
        "type": "string",
        "example": "UboNatural",
        "description": "The Customer Type"
      },
      "PersonalDetailsGetResponse": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "description": "The UBO's first name",
            "nullable": true,
            "example": "Linn"
          },
          "middleName": {
            "type": "string",
            "description": "The UBO's middle name",
            "nullable": true,
            "example": "Hernan"
