      },
      "UboNonNaturalCustomerGetResponse": {
        "type": "object",
        "properties": {
          "type": {
            "$ref": "#/components/schemas/CustomerType"
          },
          "id": {
            "type": "string",
            "description": "The unique identifier of the customer.",
            "format": "uuid"
          },
          "institutionId": {
            "type": "string",
            "description": "The unique identifier of the institution associated with this customer.",
            "format": "uuid"
          },
          "status": {
            "$ref": "#/components/schemas/CustomerStatus"
          },
          "personalDetails": {
            "$ref": "#/components/schemas/PersonalDetailsGetResponse"
          },
          "businessDetails": {
            "$ref": "#/components/schemas/UboCompanyDetailsGetResponse"
          }
        },
        "additionalProperties": false,
        "description": "The UBO Non-Natural Customer type response body"
      },
      "UboNaturalCustomerPatchResponse": {
        "type": "object",
        "properties": {
          "customerId": {
            "type": "string",
            "description": "The unique identifier of the UBO Natural customer.",
            "format": "uuid"
          },
          "personalDetails": {
            "$ref": "#/components/schemas/UboNaturalPatchModel"
          },
          "businessDetails": {
            "$ref": "#/components/schemas/UboNonNaturalPatchModel"
          }
        },
        "additionalProperties": false,
        "description": "Response summarising which fields were changed by a UBO Natural PATCH.\r\nContains only sections and properties that were changed."
      },
      "UboNaturalPatchModel": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "description": "Updated first name (if changed).",
            "nullable": true,
            "example": "Lynne"
          },
          "middleName": {
            "type": "string",
            "description": "Updated middle name (if changed).",
            "nullable": true,
            "example": "Hernandez"
          },
          "surname": {
            "type": "string",
            "description": "Updated surname (if changed).",
            "nullable": true,
            "example": "Sanchez"
          },
          "countryOfResidence": {
            "$ref": "#/components/schemas/CountryCode"
          },
          "dateOfBirth": {
            "type": "string",
            "description": "Updated date of birth (if changed).",
            "format": "date",
            "nullable": true,
            "example": "1978-05-21"
          }
        },
        "additionalProperties": false,
        "description": "Will only be provided if the UBO you updated is a natural person."
      },
      "UboNonNaturalCustomerPatchResponse": {
        "type": "object",
        "properties": {
          "customerId": {
            "type": "string",
            "description": "The unique identifier of the UBO customer.",
            "format": "uuid"
          },
          "businessDetails": {
            "$ref": "#/components/schemas/UboNonNaturalPatchModel"
          }
        },
        "additionalProperties": false,
        "description": "Response summarising which fields were changed by a Ubo PATCH."
      },
      "UboNonNaturalPatchModel": {
        "type": "object",
        "properties": {
          "registeredName": {
            "type": "string",
            "description": "Updated registered name (if changed).",
            "nullable": true,
            "example": "Smitti and Co"
          },
          "countryOfRegistration": {
            "$ref": "#/components/schemas/CountryCode"
          }
        },
        "additionalProperties": false,
        "description": "Contains business details that were updated. May only be provided if the UBO you are updating is non-natural."
      },
      "UboPatchResponse": {
        "$ref": "#/components/schemas/UboNaturalCustomerPatchResponse",
