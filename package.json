    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prettier": "prettier-standard --format '**/*.{tsx,jsx,js,ts}'",
    "test": "jest -c jest.config.js --coverage=false",
    "test:coverage": "jest -c jest.config.js",
    "test:watch": "npm run test -- --watchAll",
    "test:watch:latest": "npm run test -- --watch --onlyChanged -u",
    "test:update-snapshots": "npm run test -- -u",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open",
